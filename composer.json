    "keywords": [
        "saml", "authentication", "SimpleSAMLphp", "Neos", "Flow"
    ],
    "require": {
        "typo3/flow": "*",
        "simplesamlphp/simplesamlphp": "1.15"
    },
    "name": "swisscom/SimpleSamlServiceProvider",
    "autoload": {
        "psr-0": {
            "Swisscom\\SimpleSamlServiceProvider": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Swisscom.SimpleSamlServiceProvider"
        }
    }
}